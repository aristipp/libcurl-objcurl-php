{
   "archive" : {
      "exclude" : [
         ".*",
         "*~",
         "vendor/",
         "docs/",
         "Makefile",
         "composer.lock",
         "composer.phar",
         "composer.yaml"
      ]
   },
   "require" : {
      "ext-openssl" : "*",
      "ext-iconv" : "*",
      "ext-intl" : "*",
      "zurborg/wrap-json" : "~1.0",
      "sabre/uri" : "*",
      "php" : "~7",
      "ext-xml" : "*",
      "ext-curl" : "*",
      "zurborg/pirate-hooray" : "~1.3"
   },
   "minimum-stability" : "stable",
   "name" : "zurborg/curl-objcurl",
   "description" : "Object-orientated cURL class for PHP",
   "keywords" : [
      "curl"
   ],
   "autoload" : {
      "psr-0" : {
         "Curl" : "src/"
      }
   },
   "homepage" : "https://github.com/zurborg/libcurl-objcurl-php",
   "authors" : [
      {
         "homepage" : "https://zurborg.github.io/",
         "email" : "zurborg@cpan.org",
         "name" : "David Zurborg"
      }
   ],
   "license" : "MIT",
   "type" : "library",
   "require-dev" : {
      "phpdocumentor/phpdocumentor" : "~2",
      "evert/phpdoc-md" : "~0",
      "phpunit/phpunit" : "~5",
      "squizlabs/php_codesniffer" : "~2"
   }
}
